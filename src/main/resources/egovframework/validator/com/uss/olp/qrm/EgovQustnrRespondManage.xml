<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>

        <form name="qustnrRespondManageVO">
        			
			<field property="sexdstnCode" depends="required">
        		<arg0 key="comUssOlpQrm.regist.sexdstnCode" resource="true"/><!-- 성별 -->
			</field> 
			
			<field property="occpTyCode" depends="required">
        		<arg0 key="comUssOlpQrm.regist.occpTyCode" resource="true"/><!-- 직업 -->
			</field> 
			
			<field property="respondNm" depends="required, maxlength">
        		<arg0 key="comUssOlpQrm.regist.respondNm" resource="true"/><!-- 응답자명 -->
        		<arg1 key="50" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>50</var-value>
				</var>
			</field> 
		
		</form>

    </formset>

</form-validation>